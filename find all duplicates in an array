HASH SET METHOD
class Solution {
public:
    vector<int> findDuplicates(vector<int>& nums) {
        unordered_set<int> myset;
        vector<int> ans;
        int n=nums.size();
        for(int i=0; i<n; i++){
            if(myset.find(nums[i]) == myset.end())
            {
                myset.insert(nums[i]);
            }
            else
            {
                ans.push_back(nums[i]);
            }
        }
        return ans;
    }
};
